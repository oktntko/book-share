{
  "name": "book-share-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --verbose --watch src --exec \"node --require @swc-node/register\" src/app.ts",
    "prebuild": "rimraf dist",
    "build": "swc src --out-dir dist",
    "build:types": "tsc --project tsconfig.types.json && tsc-alias",
    "db": "npm-run-all --sequential db:*",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "SWC_NODE_PROJECT=\"tsconfig.prisma.json\" node --require @swc-node/register prisma/seed.ts",
    "lint": "eslint --ext .js,.ts src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@trpc/server": "^9.27.2",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "log4js": "^6.6.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@swc/cli": "^0.1.57",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "npm-check-updates": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.3"
  }
}
